<p id="notice"><%= notice %></p>

<h1>Data Types</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% @data_types.each do |data_type| %>
      <tr>
        <td><%= data_type.name %></td>
        <td><%= data_type.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>